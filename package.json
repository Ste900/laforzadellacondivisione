{
  "scripts": {
    "build": "npx tailwindcss -i ./css/main.css -o ./css/app.css --minify",
    "watch": "npx tailwindcss -i ./css/main.css -o ./css/app.css --watch",
    "compile": "html-includes --src ./pages --dest .",
    "compile:min": "html-includes --src ./pages --dest . --minify",
    "compile:watch": "html-includes --src ./pages --dest . --watch",
    "prod": "npm run compile && npm run build"
  },
  "dependencies": {
    "html-includes": "^4.4.1",
    "tailwindcss": "^3.0.24"
  }
}
